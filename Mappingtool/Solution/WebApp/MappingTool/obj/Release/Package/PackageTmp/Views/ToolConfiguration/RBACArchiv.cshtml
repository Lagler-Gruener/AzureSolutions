@model MappingTool.Models.listrbacpermconfig

<div class="jumbotron HeaderStyle">
    <h1>RBAC Archiv</h1>
    <p class="lead">In that section each admin can see the current RBAC archiv configuration.</p>
</div>
<hr />

@using (Html.BeginForm())
{
    <center>
        <div class="MTGridSizeFull">
            <div style="text-align:right">
                @Html.ValidationSummary(false, "", new { @class = "text-danger" })
            </div>
            <div class="well MTGridHeader">
                <div class="form-inline MTGridInner">
                    <div class="form-group" style="padding-right:10px">
                        <h4>Azure archived permissions</h4>
                    </div>
                    <div class="form-group MTGridInnerButtonright">
                        
                    </div>
                </div>
            </div>
            @Html.MVCGrid("RBACConfigArchiv")
        </div>
    </center>
}